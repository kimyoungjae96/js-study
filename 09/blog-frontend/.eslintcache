));\n  }, [dispatch]);\n\n  useEffect(() => {\n    if (authError) {\n      if (authError.response.status === 409) {\n        setError('이미 존재하는 계정명입니다.');\n        return;\n      }\n      setError('회원가입 실패');\n      return;\n    }\n    if (auth) {\n      console.log('회원가입 성공');\n      console.log(auth);\n      dispatch(check());\n    }\n  }, [auth, authError, dispatch]);\n\n  useEffect(() => {\n    if (user) {\n      history.push('/');\n      try {\n        localStorage.setItem('user', JSON.stringify(user));\n      } catch (e) {\n        console.log('localStorage is not working');\n      }\n    }\n  }, [user]);\n  return (\n    <AuthForm\n      type=\"register\"\n      form={form}\n      onChange={onChange}\n      onSubmit={onSubmit}\n      error={error}\n    />\n  );\n};\n\nexport default withRouter(RegisterForm);\n",["173","174"],"/Users/crerar/Desktop/github/js-study/09/blog-frontend/src/modules/loading.js",[],"/Users/crerar/Desktop/github/js-study/09/blog-frontend/src/lib/api/client.js",[],["175","176"],"/Users/crerar/Desktop/github/js-study/09/blog-frontend/src/modules/user.js",[],"/Users/crerar/Desktop/github/js-study/09/blog-frontend/src/lib/api/auth.js",[],"/Users/crerar/Desktop/github/js-study/09/blog-frontend/src/pages/PostPage.js",[],"/Users/crerar/Desktop/github/js-study/09/blog-frontend/src/components/common/Header.js",[],"/Users/crerar/Desktop/github/js-study/09/blog-frontend/src/containers/common/HeaderContainer.js",[],"/Users/crerar/Desktop/github/js-study/09/blog-frontend/src/components/write/Editor.js",[],"/Users/crerar/Desktop/github/js-study/09/blog-frontend/src/components/common/Responsive.js",[],"/Users/crerar/Desktop/github/js-study/09/blog-frontend/src/components/write/TagBox.js",["177"],"/Users/crerar/Desktop/github/js-study/09/blog-frontend/src/components/write/WriteActionButtons.js",[],"/Users/crerar/Desktop/github/js-study/09/blog-frontend/src/modules/write.js",[],"/Users/crerar/Desktop/github/js-study/09/blog-frontend/src/containers/write