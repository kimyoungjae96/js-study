}\n        onChange={onChange}\n      />\n      <button onClick={onClick}>확인</button>\n    </div>\n  )\n}\n\n\nexport default EventPractice",["238","239"],"/Users/crerar/Desktop/github/js-study/09/manage-react/src/ValidationSample/ValidationSample.js",[],"/Users/crerar/Desktop/github/js-study/09/manage-react/src/ScrollBox/ScrollBox.js",[],"/Users/crerar/Desktop/github/js-study/09/manage-react/src/LifeCycleSample/LifeCycleSample.js",[],"/Users/crerar/Desktop/github/js-study/09/manage-react/src/ErrorBoundary/ErrorBoundary.js",[],"/Users/crerar/Desktop/github/js-study/09/manage-react/src/Counter/Counter.js",[],"/Users/crerar/Desktop/github/js-study/09/manage-react/src/Info/Info.js",["240","241"],"import React, { useReducer } from \"react\";\nimport useInputs from \"../UseInputs/useInputs\";\n\nfunction reducer(state, action) {\n  return {\n    ...state,\n    [action.name]: action.value,\n  };\n}\n\nconst Info = () => {\n  const [state, onChange] = useInputs({\n    name: \"\",\n    nickname: \"\",\n  });\n  const { name, nickname } = state;\n  return (\n    <div>\n      <div>\n        <input name=\"name\" value={name} onChange={onChange} />\n        <input name=\"nickname\" value={nickname} onChange={onChange} />\n      </div>\n      <div>\n        <div>\n          <b>이름:</b> {name}\n        </div>\n        <div>\n          <b>닉네임:</b>\n          {nickname}\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default Info;\n","/Users/crerar/Desktop/github/js-study/09/manage-react/src/Average/Average.js",[],"/Users/crerar/Desktop/github/js-study/09/manage-react/src/UseInputs/useInputs.js",[],"/Users/crerar/Desktop/github/js-study/09/manage-react/src/SassComponent/SassComponent.js",[],"/Users/crerar/Desktop/github/js-study/09/manage-react/src/StyledComponents/StyledComponents.js",[],"/Users/crerar/Desktop/github/js-study/09/manage-react/src/TodoApp/App.js",["242"],"import React, { useState, useRef, useCallback, useReducer } from \"react\";\nimport TodoTemplate from \"./components/TodoTemplate\";\nimport TodoInsert from \"./components/TodoInsert\";\nimport \"./index.css\";\nimport TodoList from \"./components/TodoList\";\n\nconst createBulkTodos = () => {\n  const array = [];\n  for (let i = 1; i <= 5000; i++) {\n    array.push({\n      id: i,\n      text: `할 일 ${i}`,\n      checked: false,\n    });\n  }\n  return array;\n};\n\nconst todoReducer = (todos, action) => {\n  switch (action.type) {\n    case \"INSERT\":\n      return todos.concat(action.todo);\n    case \"REMOVE\":\n      return todos.filter((todo) => todo.id !== action.id);\n    case \"TOGGLE\":\n      return todos.map((todo) =>\n        todo.id === action.id ? { ...todo, checked: !todo.checked } : todo\n      );\n    default:\n      return todos;\n  }\n};\n\nconst TodoApp = () => {\n  const [todos, dispatch] = useReducer(todoReducer, undefined, createBulkTodos);\n\n  const nextId = useRef(5001);\n  const onInsert = useCallback((text) => {\n    const todo = {\n      id: nextId.current,\n      text,\n      checked: false,\n    };\n    dispatch({ type: \"INSERT\", todo });\n    nextId.current += 1;\n  }, []);\n\n  const onRemove = useCallback((id) => {\n    dispatch({ type: \"REMOVE\", id });\n  }, []);\n\n  const onToggle = useCallback((id) => {\n    dispatch({ type: \"TOGGLE\", id });\n  }, []);\n\n  return (\n    <TodoTemplate>\n      <TodoInsert onInsert={onInsert} />\n      <TodoList todos={todos} onRemove={onRemove} onToggle={onToggle} />\n    </TodoTemplate>\n  );\n};\n\nexport default TodoApp;\n","/Users/crerar/Desktop/github/js-study/09/manage-react/src/TodoApp/components/TodoTemplate.js",[],"/Users/crerar/Desktop/github/js-study/09/manage-react/src/TodoApp/components/TodoInsert.js",[],"/Users/crerar/Desktop/github/js-study/09/manage-react/src/TodoApp/components/TodoList.js",["243"],"import React, { useCallback } from \"react\";\nimport TodoListItem from \"./TodoListItem\";\nimport \"./TodoList.scss\";\nimport { List } from \"react-virtualized\";\n\nconst